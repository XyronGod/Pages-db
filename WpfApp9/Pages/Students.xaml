        <Label Content="Имя:" HorizontalAlignment="Left" Margin="193,153,0,0" VerticalAlignment="Top" Background="White" Width="80" Height="23"/>
        <Label Content="Фамилия:" HorizontalAlignment="Left" Margin="193,181,0,0" VerticalAlignment="Top" Width="80" Background="White" Height="23"/>
        <TextBox x:Name="textboxID" HorizontalAlignment="Left" Height="24" Margin="278,124,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="ID:" HorizontalAlignment="Left" Margin="193,124,0,0" VerticalAlignment="Top" Height="24" Width="80" Background="White"/>
    </Grid>
</Page>
