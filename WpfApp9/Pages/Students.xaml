        <ComboBox x:Name="comboboxgroup" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" RenderTransformOrigin="2.6,7.156" Margin="278,209,0,0" SelectionChanged="comboboxgroup_SelectionChanged"/>
